{
  "name": "fresh-brand-ws",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation eslint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm -f pnpm-lock.yaml && pnpm install --legacy-peer-deps",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "vercel-build": "pnpm install --legacy-peer-deps && next build"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.36.0",
    "@payloadcms/db-postgres": "3.36.0",
    "@payloadcms/live-preview-react": "3.36.0",
    "@payloadcms/next": "3.36.0",
    "@payloadcms/payload-cloud": "3.36.0",
    "@payloadcms/plugin-form-builder": "3.36.0",
    "@payloadcms/plugin-nested-docs": "3.36.0",
    "@payloadcms/plugin-redirects": "3.36.0",
    "@payloadcms/plugin-search": "3.36.0",
    "@payloadcms/plugin-seo": "3.36.0",
    "@payloadcms/richtext-lexical": "3.36.0",
    "@payloadcms/richtext-slate": "3.36.0",
    "@payloadcms/storage-s3": "3.36.0",
    "@payloadcms/ui": "3.36.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/pg": "^8.15.4",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.378.0",
    "next": "14.2.4",
    "next-sitemap": "^4.2.3",
    "payload": "3.36.0",
    "pg": "^8.16.3",
    "postcss": "^8.4.47",
    "prism-react-renderer": "^2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.4",
    "sharp": "^0.34.2",
