#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/erinjerri/node_modules/.pnpm/next-sitemap@4.2.3_next@15.3.5_@playwright+test@1.50.0_react-dom@19.1.0_react@19.1.0__react@19.1.0_sass@1.89.2_/node_modules/next-sitemap/bin/node_modules:/Users/erinjerri/node_modules/.pnpm/next-sitemap@4.2.3_next@15.3.5_@playwright+test@1.50.0_react-dom@19.1.0_react@19.1.0__react@19.1.0_sass@1.89.2_/node_modules/next-sitemap/node_modules:/Users/erinjerri/node_modules/.pnpm/next-sitemap@4.2.3_next@15.3.5_@playwright+test@1.50.0_react-dom@19.1.0_react@19.1.0__react@19.1.0_sass@1.89.2_/node_modules:/Users/erinjerri/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/erinjerri/node_modules/.pnpm/next-sitemap@4.2.3_next@15.3.5_@playwright+test@1.50.0_react-dom@19.1.0_react@19.1.0__react@19.1.0_sass@1.89.2_/node_modules/next-sitemap/bin/node_modules:/Users/erinjerri/node_modules/.pnpm/next-sitemap@4.2.3_next@15.3.5_@playwright+test@1.50.0_react-dom@19.1.0_react@19.1.0__react@19.1.0_sass@1.89.2_/node_modules/next-sitemap/node_modules:/Users/erinjerri/node_modules/.pnpm/next-sitemap@4.2.3_next@15.3.5_@playwright+test@1.50.0_react-dom@19.1.0_react@19.1.0__react@19.1.0_sass@1.89.2_/node_modules:/Users/erinjerri/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/next-sitemap@4.2.3_next@15.3.5_@playwright+test@1.50.0_react-dom@19.1.0_react@19.1.0__react@19.1.0_sass@1.89.2_/node_modules/next-sitemap/bin/next-sitemap.cjs" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/next-sitemap@4.2.3_next@15.3.5_@playwright+test@1.50.0_react-dom@19.1.0_react@19.1.0__react@19.1.0_sass@1.89.2_/node_modules/next-sitemap/bin/next-sitemap.cjs" "$@"
fi
