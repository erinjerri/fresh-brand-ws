{
  "version": 2,
  "functions": {
    "src/app/(payload)/api/[...slug]/route.ts": {
      "maxDuration": 30
    },
    "src/app/(payload)/api/graphql/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_OPTIONS": "--no-deprecation",
    "ENABLE_EXPERIMENTAL_COREPACK": "1"
  },
  "framework": "nextjs",
  "outputDirectory": ".next",
  "installCommand": "pnpm install --no-frozen-lockfile"
}
